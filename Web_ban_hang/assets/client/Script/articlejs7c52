function printArticleContent(n){var t=window.open("",n,"height=400,width=600"),i;t.document.write("<html><head><title>"+n+"<\/title>");t.document.write('<link rel="stylesheet" href="/Content/css/reset.css" type="text/css" />');t.document.write('<link rel="stylesheet" href="/Content/css/custom.css" type="text/css" />');t.document.write('<link rel="stylesheet" href="/Content/css/article/article.css" type="text/css" />');t.document.write("<style>body { min-width: 0; }<\/style>");t.document.write("<\/head><body>");i=$("#divArticleContent").html().replace($(".news-detail-share").html(),"");t.document.write(i);t.document.write("<\/body><\/html>");setTimeout(function(){t.document.close();t.focus();t.print();t.close()},500)}$(function(){$(".news-headbar .news-headbar-right .more").on("click",function(){$(".news-headbar .news-headbar-right .more").not(this).removeClass("open");$(this).toggleClass("open")});$(".news-detail-comment .titletabcomment ul li").on("click",function(){var n=$(this).attr("id");$(".news-detail-comment .container-comment .box").hide();$(".news-detail-comment .container-comment ."+n).show();$(this).siblings("li").removeClass("activetab");$(this).addClass("activetab")});$("body").on("click",function(n){$(n.target).closest(".news-main .news-headbar .news-headbar-right .more").length||$(".news-main .news-headbar .news-headbar-right .more").removeClass("open")});$(".thematic").hasOverflow()&&$(".thematic").addClass("more")});$.fn.hasOverflow=function(){var i=$(this),r=i.find("*"),u=r.length,n,t;return u?(n=0,t=0,r.map(function(){n=Math.max(n,$(this).outerWidth(!0));t=Math.max(t,$(this).outerHeight(!0))}),n>i.width()||t>i.height()):!1}